CXXFLAGS = `rtm-config --cflags` -I.
LDFLAGS  = `rtm-config --libs` -lm
SHFLAGS  = -shared

IDLC     = `rtm-config --idlc`
IDLFLAGS = `rtm-config --idlflags` -I`rtm-config --prefix`/include/rtm/idl
WRAPPER  = rtm-skelwrapper
WRAPPER_FLAGS = --include-dir="" --skel-suffix=Skel --stub-suffix=Stub

CXXSRC = ../HockeyLibs2/MyMath.cpp
CXXOBJ = $(CXXSRC:%.cpp=%.o)

SKEL_OBJ = 
STUB_OBJ = 
IMPL_OBJ = 
OBJS     = AHAttack.o $(CXXOBJ) $(SKEL_OBJ) $(STUB_OBJ) $(IMPL_OBJ)

.SUFFIXES : .so

all: AHAttack.so AHAttackComp

.cpp.o:
	rm -f $@
	$(CXX) $(CXXFLAGS) -c -o $@ $<

.o.so:
	rm -f $@
	$(CXX) $(SHFLAGS) -o $@ $(OBJS) $(LDFLAGS)

AHAttackComp: AHAttackComp.o $(OBJS)
	$(CXX) -o $@ $(OBJS) AHAttackComp.o $(LDFLAGS) 


clean: clean_objs clean_skelstub
	rm -f *~

clean_objs:
	rm -f $(OBJS) AHAttackComp.o AHAttack.so AHAttackComp

clean_skelstub:
	rm -f *Skel.h *Skel.cpp
	rm -f *Stub.h *Stub.cpp



AHAttack.so: $(OBJS)
AHAttack.o: AHAttack.h 	 	
AHAttackComp.o: AHAttackComp.cpp AHAttack.cpp AHAttack.h 



